<?xml version="1.0" encoding="utf-8"?>
<navigation
	xmlns:android = "http://schemas.android.com/apk/res/android"
	xmlns:app = "http://schemas.android.com/apk/res-auto"
	xmlns:tools = "http://schemas.android.com/tools"
	android:id = "@+id/navigation_graph_main"
	app:startDestination = "@id/launchesFragment">

	<fragment
		android:id = "@+id/launchesFragment"
		android:name = "ritwik.samples.spacex.ui.main.fragments.LaunchesFragment"
		android:label = "@string/view_label_launches"
		tools:layout = "@layout/fragment_launches" >

		<action
			android:id = "@+id/action_launchesFragment_to_vehicleFragment"
			app:destination = "@id/vehicleFragment"
			app:enterAnim = "@anim/nav_default_enter_anim"
			app:exitAnim = "@anim/nav_default_exit_anim"
			app:popEnterAnim = "@anim/nav_default_pop_exit_anim"
			app:popExitAnim = "@anim/nav_default_pop_enter_anim" />

	</fragment>

	<fragment
		android:id = "@+id/vehicleFragment"
		android:name = "ritwik.samples.spacex.ui.main.fragments.VehicleFragment"
		android:label = "@string/view_label_vehicles"
		tools:layout = "@layout/fragment_vehicle" />

</navigation>